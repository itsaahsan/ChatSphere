# API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_SOCKET_URL=http://localhost:5000

# App Configuration
REACT_APP_NAME=ChatSphere
REACT_APP_VERSION=1.0.0

# Features Toggle (Optional)
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=true
REACT_APP_ENABLE_VOICE_CALLS=true
REACT_APP_ENABLE_VIDEO_CALLS=true
REACT_APP_ENABLE_STATUS=true

# File Upload Limits
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/gif,image/webp
REACT_APP_ALLOWED_DOCUMENT_TYPES=application/pdf,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document

# Analytics (Optional)
# REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X
# REACT_APP_SENTRY_DSN=your_sentry_dsn

# Environment
REACT_APP_ENV=development
